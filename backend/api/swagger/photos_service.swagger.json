    "application/json"
  ],
  "paths": {
    "/v1/photos/groups": {
      "get": {
        "summary": "Получение списка групп фотографий",
        "operationId": "PhotosService_GetPhotoGroups",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PaginatedPhotoGroups"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "per_page",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "PhotosService"
        ]
      }
    },
    "/v1/photos/groups/main_photo": {
      "post": {
        "summary": "Изменить главную фотографию группы",
        "operationId": "PhotosService_SetMainPhotoGroup",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SetMainPhotoGroupRequest"
            }
          }
        ],
        "tags": [
          "PhotosService"
        ]
      }
    },
    "/v1/photos/groups/{group_id}": {
      "get": {
        "summary": "Получение группы фотографий",
        "operationId": "PhotosService_GetPhotoGroup",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PhotoGroupData"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "group_id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "PhotosService"
        ]
      }
    },
    "/v1/tags": {
      "get": {
        "summary": "Получение списка категорий тегов",
        "operationId": "TagsService_GetTagsCategory",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GetTagsCategoryResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "per_page",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "TagsService"
        ]
      }
    }
  },
