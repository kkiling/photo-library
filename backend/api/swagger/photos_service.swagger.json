{
  "swagger": "2.0",
  "info": {
    "title": "Photo library API",
    "version": "0.1"
  },
  "tags": [
    {
      "name": "PhotosService"
    }
  ],
  "host": "localhost:8080",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/photos/groups": {
      "post": {
        "summary": "Получение списка групп фотографий",
        "operationId": "PhotosService_GetPhotoGroups",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GetPhotoGroupsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/GetPhotoGroupsRequest"
            }
          }
        ],
        "tags": [
          "photo"
        ]
      }
    },
    "/v1/photos/{url}": {
      "get": {
        "summary": "Загрузка фотографии",
        "operationId": "PhotosService_GetImage",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GetImageResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "url",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "photo"
        ]
      }
    }
  },
  "definitions": {
    "Any": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "GetImageResponse": {
      "type": "object",
      "properties": {
        "image": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "GetPhotoGroupsRequest": {
      "type": "object",
      "properties": {
        "page": {
          "type": "integer",
          "format": "int32"
        },
        "per_page": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "GetPhotoGroupsResponse": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/PhotoGroup"
          }
        },
        "total_items": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "Photo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "PhotoGroup": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "main_photo": {
          "$ref": "#/definitions/Photo"
        },
        "photos_count": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "Status": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/Any"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}
