{
  "swagger": "2.0",
  "info": {
    "title": "Photo library API",
    "version": "0.1"
  },
  "tags": [
    {
      "name": "PhotosService"
    }
  ],
  "host": "localhost:8080",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/photos/check-hash": {
      "post": {
        "summary": "Проверка по хешу фотографии, загружена она или нет",
        "operationId": "PhotosService_CheckHashPhoto",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/CheckHashPhotoResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CheckHashPhotoRequest"
            }
          }
        ],
        "tags": [
          "photos"
        ]
      }
    },
    "/v1/photos/upload": {
      "post": {
        "summary": "Загрузка новой фотографии",
        "operationId": "PhotosService_UploadPhoto",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/UploadPhotoResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/Status"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/UploadPhotoRequest"
            }
          }
        ],
        "tags": [
          "photos"
        ]
      }
    }
  },
  "definitions": {
    "Any": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "CheckHashPhotoRequest": {
      "type": "object",
      "properties": {
        "hash": {
          "type": "string"
        }
      }
    },
    "CheckHashPhotoResponse": {
      "type": "object",
      "properties": {
        "already_uploaded": {
          "type": "boolean"
        },
        "uploaded_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Status": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/Any"
          }
        }
      }
    },
    "UploadPhotoRequest": {
      "type": "object",
      "properties": {
        "paths": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "body": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "UploadPhotoResponse": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "hash": {
          "type": "string"
        },
        "uploaded_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}
